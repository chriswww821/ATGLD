@section Scripts
{
	<script>
		//Semantic Ui Modals.
		$('.node').on('click', function () {
			$('.ui.modal#' + $(this).attr('data-object')).modal('show');
		});

		//Changing path colors on hover to show clickable items.
		$('.node').hover(function () {
			$(this).find('.cls-11, .cls-12, .cls-20').css('transition', 'all 0.2s ease-out');
			$(this).find('.cls-12').css('fill', '#0e6d77');
		}, function () {
			$(this).find('.cls-12').css('fill', '#15a5b6');
		});

		$('.node').hover(function () {
			$(this).find('path.cls-7').css('transition', 'all 0.2s ease-out');
			$(this).find('path.cls-7').css('fill', '#1c3a3a');
			$(this).find('.cls-11').css('transition', 'all 0.2s ease-out');
			$(this).find('.cls-11').css('fill', '#0e6d77');
		}, function () {
			$(this).find('path.cls-7').css('fill', '#4d6060');
			$(this).find('.cls-11').css('fill', '#15a5b6');
		});
	</script>
}
